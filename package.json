{
  "name": "okna-na-veka-website",
  "version": "1.0.0",
  "description": "Website for windows installation company in Kaluga",
  "main": "index.html",
  "source": "src/index.html",
  "scripts": {
    "dev": "parcel src/index.html --port 3000",
    "build": "parcel build src/index.html --dist-dir dist --public-url ./",
    "preview": "parcel serve dist --port 3000",
    "clean": "rm -rf dist .parcel-cache",
    "start": "npm run dev",
    "lint": "echo 'Linting not configured with Parcel'",
    "format": "prettier --write 'src/**/*.{html,scss,css,js}'",
    "test": "npm run build && echo 'Build test passed!'"
  },
  "keywords": [
    "windows",
    "installation",
    "kaluga",
    "pvc",
    "veka",
    "rehau",
    "whs"
  ],
  "author": "Denis Tockiy",
  "license": "MIT",
  "devDependencies": {
    "parcel": "^2.12.0",
    "prettier": "^3.1.0",
    "@parcel/transformer-sass": "^2.12.0"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ]
}
